{
  "expo": {
    "name": "Festa",
    "slug": "festaappV2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/festa-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/festa-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/festa-icon.png",
      "bundleIdentifier": "com.anonymous.festaappV2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/festa-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.festaappV2"
    },
    "web": {
      "favicon": "./assets/festa-icon.png"
    }
  }
}
